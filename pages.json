{

    "easycom": {
        "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
    },
    "pages": [{
            "path": "pages/index/index",
            "style": {
                "navigationBarTitleText": "",
                "navigationBarBackgroundColor": "#FFFFFF",
                "navigationStyle": "custom"
            }
        },
        {
            "path": "pages/index/home/index",
            "style": {
                "navigationBarTitleText": "首页",
                "navigationBarBackgroundColor": "#FFFFFF",
                "navigationStyle": "custom",
                "onReachBottomDistance": 150,
                "backgroundColor": "#FFFFFF",
                "enablePullDownRefresh": true
            }
        },

        {
            "path": "pages/index/user/index",
            "style": {
                "navigationBarTitleText": "个人中心",
                "navigationBarBackgroundColor": "#FFFFFF"
            }
        },

        {
            "path": "pages/index/collection/index",
            "style": {
                "navigationBarTitleText": "收藏",
                "navigationBarBackgroundColor": "#FFFFFF"
            }
        }

    ],


    "subPackages": [{
            "root": "pages/subPack",
            "pages": [{
                    "path": "merchant/index",
                    "style": {
                        "navigationBarTitleText": "",
                        "navigationBarBackgroundColor": "#FFFFFF",
                        // #ifdef MP-WEIXIN
                        "navigationStyle": "custom",
                        // #endif
                        "enablePullDownRefresh": true

                    }
                },
                {
                    "path": "cart/cart",
                    "style": {
                        "navigationBarTitleText": "购物车",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                }, {
                    "path": "commodity/commodity",
                    "style": {
                        "navigationBarTitleText": "商品详情",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                }, {
                    "path": "search-page/search-page",
                    "style": {
                        "navigationBarTitleText": "搜索",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                },


                {
                    "path": "categroy/index",
                    "style": {
                        "navigationBarTitleText": "分类",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                },


                {
                    "path": "categroy/searchPage",
                    "style": {
                        "navigationBarTitleText": "搜索页",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                },
                {
                    "path": "merchant/storeDetails",
                    "style": {
                        "navigationBarTitleText": "店铺详情",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                }, {
                    "path": "merchant/storeList",
                    "style": {
                        "navigationBarTitleText": "店铺列表",
                        "navigationBarBackgroundColor": "#FFFFFF",
                        "navigationStyle": "custom"
                    }
                }, {
                    "path": "merchant/commodityDetail",
                    "style": {
                        "navigationBarTitleText": "商品详情",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                },
                {
                    "path": "merchant/articleDetail",
                    "style": {
                        "navigationBarTitleText": "文章详情",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                }

            ]
        },
        {
            "root": "pages/order",
            "pages": [

                {
                    "path": "editAddress/editAddress",
                    "style": {
                        "navigationBarTitleText": "地址信息",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                }, {
                    "path": "delivery-address/delivery-address",
                    "style": {
                        "navigationBarTitleText": "收货地址",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                }, {
                    "path": "order-detail/order-detail",
                    "style": {
                        "navigationBarTitleText": "订单详情",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                },
                {
                    "path": "order-list/order-list",
                    "style": {
                        "navigationBarTitleText": "订单列表",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                }

            ]
        }, {
            "root": "pages/public",
            "pages": [{
                    "path": "login",
                    "style": {
                        "navigationBarTitleText": "登录",
                        "navigationBarBackgroundColor": "#FFFFFF",
                        "app-plus": {
                            "bounce": "vertical",
                            "titleNView": {
                                "type": "transparent"
                            }
                        }
                    }
                },
                {
                    "path": "agreement",
                    "style": {
                        "navigationBarTitleText": "协议",
                        "navigationBarBackgroundColor": "#FFFFFF",
                        "app-plus": {
                            "bounce": "vertical",
                            "titleNView": {
                                "type": "transparent"
                            }
                        }
                    }
                },
                {
                    "path": "register",
                    "style": {
                        "navigationBarTitleText": "注册",
                        "navigationBarBackgroundColor": "#FFFFFF",
                        "app-plus": {
                            "bounce": "vertical",
                            "titleNView": {
                                "type": "transparent"
                            }
                        }
                    }
                }
            ]
        }, {
            "root": "pages/mine",
            "pages": [{
                    "path": "personalData",
                    "style": {
                        "navigationBarTitleText": "个人中心",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                },

                {
                    "path": "helpCenter",
                    "style": {
                        "navigationBarTitleText": "帮助中心",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                },
                {
                    "path": "about",
                    "style": {
                        "navigationBarTitleText": "关于我们",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                }


            ]
        }, {
            "root": "pages/coupon",
            "pages": [{
                    "path": "receive",
                    "style": {
                        "navigationBarTitleText": "领取积分",
                        "navigationBarBackgroundColor": "#FFFFFF"
                    }
                }


            ]
        }
    ],


    "preloadRule": {
        // 当我们进入了pages/index/index页面以后就会预下载pages/subPack分包
        "pages/index/index": {
            "network": "all", //在指定网络下预下载，可选值为：all（不限网络）、wifi（仅wifi下预下载）
            "packages": ["pages/subPack", "pages/order", "pages/public", "pages/mine", "pages/coupon"] //入页面后预下载分包
        }

    },


    "globalStyle": {
        "navigationBarTextStyle": "black",
        "navigationBarBackgroundColor": "#FFFFFF",
        "backgroundColor": "#F8F8F8",
        "pageOrientation": "portrait",
        "mp-360": {
            "navigationStyle": "custom"
        },
        "app-plus": {
            "background": "#efeff4"
        }
    },
    "tabBar": {
        "color": "#333333",
        "selectedColor": "#333333",
        "borderStyle": "black",
        "backgroundColor": "#F8F8F8",
        "list": [{
                "pagePath": "pages/index/home/index",
                "iconPath": "static/img/tab_home_selected.png",
                "selectedIconPath": "static/img/tab_home_selectedHl.png",
                "text": "首页"
            },
            {
                "pagePath": "pages/index/collection/index",
                "iconPath": "static/img/tab_lobby_selected.png",
                "selectedIconPath": "static/img/tab_lobby_selectedHl.png",
                "text": "我的收藏"
            }, {
                "pagePath": "pages/index/user/index",
                "iconPath": "static/img/tab_user_selected.png",
                "selectedIconPath": "static/img/tab_user_selectedHl.png",
                "text": "个人中心"
            }
        ]
    }
}